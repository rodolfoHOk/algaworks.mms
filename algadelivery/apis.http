    "name": "João Silva",
    "phone": "11999998888",
    "complement": "Apto 10"
  },
  "recipient": {
    "zipCode": "01001-000",
    "street": "Rua da Consolação",
    "number": "123",
    "name": "João Silva",
    "phone": "11999998888",
    "complement": "Apto 10"
  },
